{
  "manifest_version": 3,
  "name": "東工大ポータル自動入力",
  "version": "1.1",
  "description": "東工大ポータルのpasswordとマトリクスコードを自動入力します",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://portal.nap.gsic.titech.ac.jp/GetAccess/*"],
      "js": ["content.js"]
    }
  ],
  //アイコンを指定
  "icons": {
    "16" : "icon/icon16.png",
    "48" : "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "author": "U.N.ABC"
}
